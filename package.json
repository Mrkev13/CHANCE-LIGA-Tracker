{
  "name": "fortuna-liga-tracker",
  "version": "1.0.0",
  "description": "Webová aplikace pro sledování české fotbalové ligy FORTUNA:LIGA",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev:server": "nodemon server/index.js",
    "dev:client": "cd client && npm start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "install:all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "test": "cd client && npm test -- --watchAll=false"
  },
  "keywords": [
    "fotbal",
    "liga",
    "fortuna",
    "česká",
    "liga",
    "živé",
    "skóre"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^9.1.3"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  }
}
